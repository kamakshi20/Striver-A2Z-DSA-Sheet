class Solution {

    void printTriangle(int n) {
        // code here
        
        
        for(int i=1; i<=n; i++){
            char ch = (char)(64 + n);
        
            for(int j=1; j<=i; j++){
                System.out.print(ch + " ");
                ch--;
            }
            System.out.println();
        }
    }
}
